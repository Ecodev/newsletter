{
    "name": "ecodev/newsletter",
    "type": "typo3-cms-extension",
    "version": "2.5.2",
    "description": "Send any pages as Newsletter and provide statistics on opened emails and clicked links.",
    "homepage": "https://github.com/ecodev/newsletter",
    "license": "GPL-2.0+",
    "keywords": ["TYPO3 CMS", "newsletter", "email", "statistics"],
    "require": {
        "ext-mcrypt": "*"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.4"
    },
	"autoload": {
		"psr-4": {
			"Ecodev\\Newsletter\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ecodev\\Newsletter\\Tests\\": "Tests/"
		}
	}
}
